    "components": {
      "schemas": {
        "AccountCategory": {
          "enum": [
            1,
            2,
            3,
            4,
            5
          ],
          "type": "integer",
          "format": "int32"
        },
        "Address": {
          "required": [
            "country"
          ],
          "type": "object",
          "properties": {
            "id": {
              "type": "integer",
              "format": "int32"
            },
            "metaData": {
              "$ref": "#/components/schemas/MetaData"
            },
            "street": {
              "type": "string",
              "nullable": true
            },
            "city": {
              "type": "string",
              "nullable": true
            },
            "state": {
              "type": "string",
              "nullable": true
            },
            "postalCode": {
              "pattern": "^\\d{5}(-\\d{4})?$",
              "type": "string",
              "nullable": true
            },
            "country": {
              "minLength": 1,
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        "BasicPay": {
          "type": "object",
          "properties": {
            "minmumPay": {
              "type": "number",
              "format": "double",
              "nullable": true
            },
            "maximumPay": {
              "type": "number",
              "format": "double",
              "nullable": true
            },
            "currency": {
              "type": "string",
              "nullable": true
            }
          },
          "additionalProperties": false
        },
        "Company": {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer",
              "format": "int32"
            },
            "metaData": {
              "$ref": "#/components/schemas/MetaData"
            },
            "companyName": {
              "type": "string",
              "nullable": true
            },
            "profile": {
              "$ref": "#/components/schemas/CompanyProfile"
            }
          },
          "additionalProperties": false
        },
        "CompanyProfile": {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer",
              "format": "int32"
            },
            "metaData": {
              "$ref": "#/components/schemas/MetaData"
            },
            "profileInfo": {
              "type": "string",
              "nullable": true
            },
            "companyAddress": {
              "$ref": "#/components/schemas/Address"
            },
            "contactPhone": {
              "$ref": "#/components/schemas/Phone"
            },
            "contactEmail": {
              "$ref": "#/components/schemas/Email"
            },
            "website": {
              "type": "string",
              "nullable": true
            },
            "industryType": {
              "$ref": "#/components/schemas/IndustryCategory"
            }
          },
          "additionalProperties": false
        },
        "Education": {
          "required": [
            "degree",
            "graduationDate",
            "institution",
            "major"
          ],
          "type": "object",
          "properties": {
            "id": {
              "type": "integer",
              "format": "int32"
            },
            "metaData": {
              "$ref": "#/components/schemas/MetaData"
            },
            "degree": {
              "minLength": 1,
              "type": "string"
            },
            "major": {
              "minLength": 1,
              "type": "string"
            },
            "institution": {
              "minLength": 1,
              "type": "string"
            },
            "graduationDate": {
              "type": "string",
              "format": "date"
            },
            "cgpa": {
              "maximum": 4,
              "minimum": 0,
              "type": "number",
              "format": "double"
            }
          },
          "additionalProperties": false
        },
        "Email": {
          "required": [
            "emailAddress",
            "emailType"
          ],
          "type": "object",
          "properties": {
            "id": {
              "type": "integer",
              "format": "int32"
            },
            "emailType": {
              "$ref": "#/components/schemas/EmailCategory"
            },
            "emailAddress": {
              "minLength": 1,
              "type": "string"
            }
          },
          "additionalProperties": false
        },
        "EmailCategory": {
          "enum": [
            "Personal",
            "Work",
            "Other"
          ],
          "type": "string"
        },
        "Experience": {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer",
              "format": "int32"
            },
            "metaData": {
              "$ref": "#/components/schemas/MetaData"
            },
            "experienceLevel": {
              "$ref": "#/components/schemas/ExperienceLevel"
            }
          },
          "additionalProperties": false
        },
        "ExperienceLevel": {
          "enum": [
            "EntryLevel",
            "Intermediate",
            "Senior",
            "Lead",
            "Principal",
            "Executive",
            "Others"
          ],
          "type": "string"
        },
        "IndustryCategory": {
          "enum": [
            "Technology",
            "Healthcare",
            "Finance",
            "Manufacturing",
            "Others"
          ],
          "type": "string"
        },
        "JobEntity": {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer",
              "format": "int32"
            },
            "metaData": {
              "$ref": "#/components/schemas/MetaData"
            },
            "companyDescription": {
              "$ref": "#/components/schemas/Company"
            },
            "jobTitle": {
              "type": "string",
              "nullable": true
            },
            "jobDescription": {
              "type": "string",
              "nullable": true
            },
            "jobType": {
              "type": "string",
              "nullable": true
            },
            "jobAddress": {
              "$ref": "#/components/schemas/Address"
            },
            "vacancies": {
              "type": "integer",
              "format": "int32",
              "nullable": true
            },
            "basicPay": {
              "$ref": "#/components/schemas/BasicPay"
            },
            "functionalArea": {
              "type": "string",
              "nullable": true
            },
            "skillsRequired": {
              "$ref": "#/components/schemas/Skill"
            },
            "requiredQualification": {
              "$ref": "#/components/schemas/Qualification"
            },
            "requiredExperience": {
              "$ref": "#/components/schemas/Experience"
            },
            "preferredQualifications": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "nullable": true
            },
            "jobResponsibilities": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "nullable": true
            },
            "benefits": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "nullable": true
            },
            "tags": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "nullable": true
            },
            "workEnvironment": {
